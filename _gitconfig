[user]
	name = Ed Moon
	email = edwardsmoon@gmail.com
[push]
	default = simple

[alias]
  st = status
  ci = commit
  br = branch
  ba = branch --all
  co = checkout
  df = diff
  lg = log -p
  undo = reset --hard
  files = diff-tree --no-commit-id --name-only -r 
  dtag = !sh -c 'git push origin :refs/tags/$1' -
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all

[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto

[core]
	excludesfile = ~/.gitignore
